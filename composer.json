{
  "name": "flipboxfactory/craft-psr3",
  "description": "PSR-3 Logger for Craft CMS",
  "version": "2.0.0",
  "keywords": [
    "psr-3",
    "psr3",
    "craft",
    "craft cms",
    "yii2"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Flipbox Factory",
      "email": "hello@flipboxfactory.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "psr/log": "^3.0",
    "craftcms/cms": "~3.0|~4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "squizlabs/php_codesniffer": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "flipbox\\craft\\psr3\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "flipbox\\craft\\psr3\\tests\\": "tests/src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}